    "mean = np.reshape(f.eval(session=tf.Session()), -1)\n",
    "covariance = v.eval(session=tf.Session())\n",
    "sample = np.random.multivariate_normal(mean, covariance)\n",
    "\n",
    "err = np.abs(sample - y_test_raw.as_matrix())\n",
    "err.mean()"
